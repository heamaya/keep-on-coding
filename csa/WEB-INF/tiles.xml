	<definition name="HomePresentationAction" extends="PresentationAction">
		<put-attribute name="title" value="Soil and Water Conservation" />
		<put-attribute name="description" value="Professional consultancy and work direction of Soil and Water Conservation Services: construction of terraces, dikes or microreservoirs, gully recovery, Crop Rotation Technic, etc. In order to controling and diminishing water erosion." />
		<put-attribute name="content" value="/WEB-INF/jsp/visitor/homePresentation.jsp" />
	</definition>
	<definition name="NewsPresentationAction" extends="PresentationAction">
		<put-attribute name="title" value="News about Soil and Water Conservation Services" />
		<put-attribute name="description" value="News about Soil and Water Conservation Services: construction of terraces, dikes or small dams, gully recovery, Crop Rotation Technic, etc. In order to controling and diminishing water erosion." />
		<put-attribute name="content" value="/WEB-INF/jsp/visitor/newsPresentation.jsp" />
	</definition>
	<definition name="DikePresentationAction" extends="PresentationAction">
		<put-attribute name="title" value="Dikes, Small Dams" />
		<put-attribute name="description" value="Soil and Water Conservation Services: dikes or small dams technology, are temporal retaining structures built in rural properties or public or internal roads in rural areas in order to contain and gradually release the water that runs off after precipitation to control and diminish water erosion." />		
		<put-attribute name="content" value="/WEB-INF/jsp/visitor/dikePresentation.jsp" />
	</definition>
	<definition name="TerracePresentationAction" extends="PresentationAction">
		<put-attribute name="title" value="Terraces" />
		<put-attribute name="description" value="Soil and Water Conservation Services: terraces are a set of small, parallel and temporal retaining structures built in rural properties as a system, perpendicular to the natural slope of the land respecting its topography, in order to retain the water that runs off after rainfall in farms to control water erosion and increase crop yield." />
		<put-attribute name="content" value="/WEB-INF/jsp/visitor/terracePresentation.jsp" />
	</definition>
	<definition name="ChannelPresentationAction" extends="PresentationAction">
		<put-attribute name="title" value="Channels" />
		<put-attribute name="description" value="Soil and Water Conservation Services: channels consist in filling natural runoff paths in places eroded by water runoff where possible, and if necessary the design and construction of drains to convey precipitation runoff strategically along the farm in order to control and diminish water erosion." />
		<put-attribute name="content" value="/WEB-INF/jsp/visitor/channelPresentation.jsp" />
	</definition>
	<definition name="GullyRecoveryPresentationAction" extends="PresentationAction">
		<put-attribute name="title" value="Gullies Recovery" />
		<put-attribute name="description" value="Soil and Water Conservation Services: gully recovery consists on building structures to prevent the advance of gullies, or if possible to recover formed gullies filling them with soil." />
		<put-attribute name="content" value="/WEB-INF/jsp/visitor/gullyRecoveryPresentation.jsp" />
	</definition>
	<definition name="WhatDoWeDoPresentationAction" extends="PresentationAction">
		<put-attribute name="title" value="What Do We Do" />
		<put-attribute name="description" value="We offer professional advice, project management, and works direction of Soil and Water Conservation Services, in order to control water erosion, and make soil production sustainable in time, increasing crop yield." />
		<put-attribute name="content" value="/WEB-INF/jsp/visitor/whatDoWeDoPresentation.jsp" />
	</definition>
	<definition name="WhoAreWePresentationAction" extends="PresentationAction">
		<put-attribute name="title" value="Who Are We" />
		<put-attribute name="description" value="We are a team of professionals with extensive experience in advising and directing works of Soil and Water Conservation Services, we have the required knowledge, equipment and tools needed to solve problems caused by water erosion in rural properties and surrounding areas. The final result of a work is an optimization for the involved farm, as well as for roads, and surrounding areas, including other farms and towns." />
		<put-attribute name="content" value="/WEB-INF/jsp/visitor/whoAreWePresentation.jsp" />
	</definition>	
	<definition name="ContactUsPresentationAction" extends="PresentationAction">
		<put-attribute name="title" value="Contact Us" />
		<put-attribute name="description" value="Contact us with any questions or concerns, we are at your disposal to find solutions to preserve, protect and improve the productiveness of your farm as well as the quality of your life and of the people of its surroundings." />
		<put-attribute name="content" value="/WEB-INF/jsp/visitor/contactUsPresentation.jsp" />
	</definition>	
